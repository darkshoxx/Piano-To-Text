<XDL>

<Synthesis>
    <Hardware>
        <Component id="reactor" type="reactor"/>
        <Component id="reactor2" type="reactor"/>
    </Hardware>

    <Reagents>
        <Reagent name="h2o" role="solvent"/>
    </Reagents>

    <Procedure>
  <Add
  reagent="h2o"
  vessel="reactor"
  volume="20" />
<HeatChill
    temp="100 C"
    time="50 s"
    stir="True" />
<Transfer
    from_vessel="reactor"
    to_vessel="reactor2"
    volume="all" />

    </Procedure>
</Synthesis>

</XDL>
